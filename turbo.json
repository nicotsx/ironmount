{
	"$schema": "https://turbo.build/schema.json",
	"ui": "stream",
	"tasks": {
		"dev": {
			"cache": false,
			"persistent": true,
			"env": ["*"]
		},
		"tsc": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/"]
		}
	}
}
